{
  "name": "api-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/src/index.js",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "build": "npm run compile",
    "predev": "npm run swagger",
    "precompile": "npm run swagger",
    "swagger": "tsoa spec",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "check": "gts check",
    "prepare": "husky install && npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "precommit": "lint-staged",
    "postinstall": "husky install",
    "cowin_cron": "npm run compile && node ./build/src/scraper/cowin_cron.js"
  },
  "lint-staged": {
    "**/*.ts": [
      "gts fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "nodemonConfig": {
    "watch": [
      "."
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "clone": "^2.1.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "luxon": "^1.26.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.6",
    "morgan": "~1.9.1",
    "node-uuid": "^1.4.8",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.7.0",
    "winston": "^3.3.3",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/node-uuid": "^0.0.28",
    "@types/clone": "^2.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/luxon": "^1.26.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^6.0.2",
    "dotenv": "^8.2.0",
    "gts": "^3.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
